#include<bits/stdc++.h>
using namespace std;

void solve(){
    long long n, s;
    cin>>n>>s;

    long long ans = s / (n * n);

    cout<<ans<<endl;
}


int32_t main(){
    ios :: sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);

    int t;
    cin>>t;

    while(t--){
        solve();
    }

    return 0;
}
