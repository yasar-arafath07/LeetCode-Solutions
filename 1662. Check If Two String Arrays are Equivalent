class Solution {
public:
    bool arrayStringsAreEqual(vector<string>& word1, vector<string>& word2) {
        string a;
        string b;
        
        for(auto i : word1){
            a += i;  // a = a + i;
        }
        
        for(auto j : word2){
            b += j;  // b = b + j;
        }
        if(a == b){
            return true;
        }
        return false;
    }
};
